<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Cucumber-Report">
    <test name="Juno-Alerting-AutomationReport">             
       <method-selectors>
              <method-selector>
                    <script language="beanshell"> 
                    <![CDATA[ 
                           groups.containsKey("testautomationtest") 
                    ]]>
              </script>
              </method-selector>
       </method-selectors>                                
       <packages>
              <package name="continuum.cucumber.testRunner" />              
       </packages>
	</test>      
</suite>

